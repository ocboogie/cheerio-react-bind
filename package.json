{
  "name": "cheerio-react-bind",
  "version": "0.1.4",
  "description": "Binds a Cheerio dom to its reacts dom with custom tags",
  "license": "MIT",
  "author": "ocboogie",
  "repository": "https://github.com/ocboogie/cheerio-react-bind",
  "main": "./lib/cheerio-react-bind.js",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "format": "npm run lint -- --fix",
    "build": "cross-env NODE_ENV=development webpack",
    "prod": "cross-env NODE_ENV=production webpack",
    "test": "jest"
  },
  "keywords": [
    "react",
    "cheerio",
    "bind",
    "binding",
    "dom",
    "virtual-dom"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/*.js?(x)"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "prettier": "^1.8.1",
    "react-test-renderer": "^16.0.0",
    "unminified-webpack-plugin": "^1.4.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "react": "^16.0.0"
  }
}
