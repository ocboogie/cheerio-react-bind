{
  "name": "cheerio-react-bind",
  "version": "1.1.1",
  "description": "Binds a Cheerio dom to its reacts dom with custom tags",
  "keywords": [
    "react",
    "cheerio",
    "bind",
    "binding",
    "dom",
    "virtual-dom"
  ],
  "main": "./dist/cheerio-react-bind.js",
  "typings": "dist/types/cheerio-react-bind.d.ts",
  "files": [
    "dist"
  ],
  "author": "Eamon mikulec <boogie@mikulec.com>",
  "repository": "https://github.com/ocboogie/cheerio-react-bind",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "tslint -t codeFrame ./{src,tests}/**/*.ts{,x}",
    "format": "prettier --write ./{src,tests}/**/*.ts{,x} & npm run lint -- --fix",
    "test": "jest",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "<rootDir>/testSetup/enzymeSetup.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.6",
    "@types/jest": "^22.0.0",
    "@types/node": "^8.5.2",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.31",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.8.1",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0"
  }
}
